<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="PrivateResource">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/list_divider"/>
            <corners
                android:topLeftRadius="@dimen/abc_control_corner_material"
                android:topRightRadius="@dimen/abc_control_corner_material"/>
        </shape>
    </item>
    <item
        android:bottom="1.25dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/list_item"/>
            <corners
                android:topLeftRadius="@dimen/abc_control_corner_material"
                android:topRightRadius="@dimen/abc_control_corner_material"/>
        </shape>
    </item>
</layer-list>
